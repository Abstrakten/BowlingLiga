<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="svin.bowlingliga" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".BowlingLeagueStartActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegisterScoreActivity"
            android:label="@string/title_activity_register_score_activiten"
            android:parentActivityName=".BowlingLeagueStartActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".BowlingLeagueStartActivity" />
        </activity>
        <activity
            android:name=".RegisterTeamActivity"
            android:label="@string/title_activity_register_team"
            android:parentActivityName=".BowlingLeagueStartActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".BowlingLeagueStartActivity" />
        </activity>
    </application>

</manifest>
